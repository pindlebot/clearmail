{
  "name": "clearmail",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/unshift/clearmail.git",
  "author": "Ben <menubar@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "UP_STAGE='' app/node_modules/.bin/parcel app/src/index.html --public-url /",
    "build": "NODE_ENV=production UP_STAGE=/staging app/node_modules/.bin/parcel build app/src/index.html --public-url /staging/",
    "start": "node server",
    "clean": "app/node_modules/.bin/rimraf dist && app/node_modules/.bin/rimraf .cache",
    "lint": "standard --fix server/**/*.js | snazzy"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "up-env": "^0.0.10"
  },
  "dependencies": {}
}
